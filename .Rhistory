g <- function(t){
1/(1-delta)*(delta^(-t)-1)
}
# weight function
w <- function(u,t){
1 / (1+g(t)*exp(-u/lambda))
}
# utility function
v <- function(x){
x^gamma/lambda
}
delta = 0.75
gamma = 0.6
lambda = 2
t_l = 16
t_s = 8
x_l = 30
x_s = 10
find_equiv <- function(u_l,u_s,t_s){
t_seq_list = t_s+c(0:1000)*0.1
obj <- abs(w(u_s,t_s)*u_s - w(u_l,t_seq_list)*u_l)
eq_idx <- which(obj == min(obj))
t_l <- t_seq_list[eq_idx]
return(t_l)
}
